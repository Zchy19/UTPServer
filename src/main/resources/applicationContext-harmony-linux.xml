<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="harmonizedConfig" class="com.macrosoft.utilities.HarmonizedConfig">
		<property name="mysqlExePath" value="/usr/bin/mysql" />
		<property name="dbUserName" value="root" />
		<property name="dbPassword" value="root" />
		<property name="restoreTemplatePath" value="/utpserver_data/utp3_template.sql" />
		<property name="cachingPath" value="/utpserver_data/utp_caching/" />
		
	</bean>
</beans>
